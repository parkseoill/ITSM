	<queryString>
		<![CDATA[SELECT  a.BIZ_REPORT_ID
, a.CREAT_CHRGR_NO
, a.RDTT
, a.BIZ_SYTM_CD
, b.BIZ_SYTM_NM
, IFNULL(a.ACRSLT_TEXT, '') AS ACRSLT_TEXT
, IFNULL(a.PLN_TEXT, '') AS PLN_TEXT
FROM (
	SELECT tbr.BIZ_REPORT_ID
	, tbr.CREAT_CHRGR_NO
	, tbr.RDTT
	, tbdr.BIZ_SYTM_CD
	, GROUP_CONCAT(tbdr.ACRSLT_TEXT SEPARATOR '\r\n\r\n') AS ACRSLT_TEXT
	, GROUP_CONCAT(tbdr.PLN_TEXT SEPARATOR '\r\n\r\n') AS PLN_TEXT
	FROM TB_BIZ_REPORT tbr
	INNER JOIN TB_BIZ_DETL_REPORT tbdr ON tbr.BIZ_REPORT_ID = tbdr.BIZ_REPORT_ID
	WHERE  (tbr.RDTT, tbr.CREAT_CHRGR_NO) IN (
		SELECT MAX(RDTT) AS RDTT
		, CREAT_CHRGR_NO
		FROM TB_BIZ_REPORT
		WHERE USE_YN = 'Y'
		AND BIZ_REPORT_KIND_CD = '01'
		AND DATE_FORMAT(RDTT, '%Y%m%d') >= REPLACE($P{jrParam1},'-','')
		AND DATE_FORMAT(RDTT, '%Y%m%d') <= REPLACE($P{jrParam2},'-','')
		GROUP BY CREAT_CHRGR_NO
	)
	GROUP BY tbdr.BIZ_SYTM_CD
) a
RIGHT OUTER JOIN TB_BIZ_SYSTEM b ON a.BIZ_SYTM_CD = b.BIZ_SYTM_CD
WHERE b.BIZ_SYTM_CD_LVL = '2'
AND b.USE_YN = 'Y'
ORDER BY b.BIZ_REPORT_OTPUT_ORDE*1;]]>
