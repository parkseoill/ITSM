package truebon.com.fai.grd.model;

import java.io.Serializable;

@SuppressWarnings("serial")
public class FailGradeVO extends FailGrade implements Serializable {
	private String g1;
	private String g2;
	private String g3;
	/* 장애영향도 이름 */
	private String failrInfldgNm;
	/* 장애긴급도 이름 */
	private String failrEmgDgrNm;
	/* 장애등급 이름 */
	private String failrGrdNm;

	
	public String getG1() {
		return g1;
	}
	public void setG1(String g1) {
		this.g1 = g1;
	}
	public String getG2() {
		return g2;
	}
	public void setG2(String g2) {
		this.g2 = g2;
	}
	public String getG3() {
		return g3;
	}
	public void setG3(String g3) {
		this.g3 = g3;
	}
	public String getFailrInfldgNm() {
		return failrInfldgNm;
	}
	public void setFailrInfldgNm(String failrInfldgNm) {
		this.failrInfldgNm = failrInfldgNm;
	}
	public String getFailrEmgDgrNm() {
		return failrEmgDgrNm;
	}
	public void setFailrEmgDgrNm(String failrEmgDgrNm) {
		this.failrEmgDgrNm = failrEmgDgrNm;
	}
	public String getFailrGrdNm() {
		return failrGrdNm;
	}
	public void setFailrGrdNm(String failrGrdNm) {
		this.failrGrdNm = failrGrdNm;
	}



